{
  "name": "functron",
  "version": "0.1.2",
  "author": "Daniel Imal <mrdanimal@gmail.com>",
  "license": "MIT",
  "description": "Functional web components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dim882/functron.git"
  },
  "bugs": {
    "url": "https://github.com/dim882/functron/issues"
  },
  "homepage": "https://github.com/dim882/functron#readme",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext html --exec 'rollup -c rollup.dev.config.js -w'",
    "dist": "rollup -c rollup.dist.config.js",
    "clean": "rm -rf dist/* dev/*",
    "tar": "yarn pack -o ./tarballs/package.tgz"
  },
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "postcss-modules": "^6.0.0",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-babel": "^6.0.4",
    "snabbdom": "^3.6.2"
  },
  "packageManager": "yarn@4.4.0+sha256.5f228cb28f2edb97d8c3b667fb7b2fdcf06c46798e25ea889dad9e0b4bc2e2c1"
}
